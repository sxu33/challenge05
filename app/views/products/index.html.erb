<h1>Products</h1>

<ul>
  <% @products.each do |product| %>
    <li>
      <a href="/products/<%= product.id %>">
        <%= product.title %>
      </a>
      <p>Category: <%= product.category.name %></p>
      <p>Price: <%= product.price %></p>
      <p>Description: <%= product.description %></p>
      <p>Stock Quantity: <%= product.stock_quantity %></p>
    </li>
  <% end %>
</ul>
